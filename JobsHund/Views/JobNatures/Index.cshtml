@model IEnumerable<jobhundClassLibrary1.JobNature>

@{
    ViewBag.Title = "Job Nature";
}



<div class="section-top-border" style="width:27cm">
    <h3 class="mb-30">All Job Nature @Html.ActionLink("Add Nature", "Create", null, new { @class = "genric-btn info", @style = "float:right;" })</h3>
    <div class="progress-table-wrap" style="width:27cm">
        <div class="progress-table">
            <div class="table-head">
                <div class="serial">#</div>
                <div class="country">Job Nature</div>
                <div class="visit">Action</div>

            </div>

            @foreach (var item in Model)
            {

                <div class="table-row">
                    <div class="serial"> @Html.DisplayFor(modelItem => item.JobNatureId)</div>
                    <div class="country">  @Html.DisplayFor(modelItem => item.JobNature1) </div>
                    <div class="visit">
                        @Html.ActionLink("Edit", "Edit", new { id = item.JobNatureId }, new { @class = "genric-btn primary" })


                    </div>

                </div>

            }

        </div>
    </div>
</div>